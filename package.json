{
  "name": "CwSServiceWorkerBuild",
  "version": "1.0.0",
  "description": "CwS PWA service worker build",
  "author": "James Chang",
  "scripts": {
    "devBuild": "node sw_build_pwa-dev.js && tar.exe cf pwa-dev.zip pwa-dev",
    "t_copyDev": "xcopy pwa-dev pwa-test /E/H/C/I",
    "t_copyManifest": "xcopy stageFiles\\test_manifest.json pwa-test\\manifest.json /Y",
    "t_swZipFolder" : "node sw_build_pwa-test.js && tar.exe cf pwa-test.zip pwa-test && RMDIR /Q /S pwa-test",
    "tBuild": "npm run t_copyDev && npm run t_copyManifest && npm run t_swZipFolder",
    "minifyJS": "uglifyjs scripts.js --compress --mangle --warn --output scripts.min.js",
    "utest": "karma start"
  },
  "dependencies": {
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.2",
    "karma-qunit": "^4.1.1",
    "karma-requirejs": "^1.1.0",
    "qunit": "^2.10.1",
    "qunitjs": "^2.4.1",
    "requirejs": "1.0.0",
    "uglify-js": "^3.10.1",
    "workbox-webpack-plugin": "^5.1.3",
    "workbox-window": "^5.1.3"
  },
  "devDependencies": {
    "workbox-build": "^5.1.3",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-jasmine": "^3.3.1",
    "karma-qunit": "^4.1.1",
    "karma-requirejs": "^1.1.0",
    "qunit": "^2.10.0",
    "qunitjs": "^2.4.1"
  }
}
